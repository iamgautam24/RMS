@model IEnumerable<WebApplication2.Models.Registration>@{    ViewBag.Title = "Index";}<h2>Index</h2><p>    @Html.ActionLink("Create New", "Create", new { area = "" }, new { @class = "navbar-brand", style = "color: Blue" })</p><table class="table">    <thead>        <tr>            <th scope="col">First Name</th>            <th scope="col">Last Name</th>            <th scope="col">Phone Number</th>            <th scope="col">Email ID</th>        </tr>    </thead>    @foreach (var item in Model)    {<tr>    <td>        @Html.DisplayFor(modelItem => item.Firstname)    </td>    <td>        @Html.DisplayFor(modelItem => item.Lastname)    </td>    <td>        @Html.DisplayFor(modelItem => item.ContactNo)    </td>    <td>        @Html.DisplayFor(modelItem => item.EmailId)    </td>    <td>        @Html.ActionLink("Edit", "Edit", new { id = item.Id, style = "color: Green" })        @Html.ActionLink("Details", "Details", new { id = item.Id })        @Html.ActionLink("Delete", "Delete", new { id = item.Id, style = "color: Red" })    </td></tr>}</table>